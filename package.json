{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "Instragram clone with Express + Prisma + React and React Native",
  "repository": "https://github.com/nomadcoders/prismagram",
  "author": "Nicolás Serrano Arévalo <itnico.las.me@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.4.5",
    "@prisma/client": "2.2.2",
    "aws-sdk": "^2.483.0",
    "copy": "^0.3.2",
    "dotenv": "^6.2.0",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.4.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nodemailer": "^5.1.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@prisma/cli": "2.2.2",
    "nodemon": "^1.18.10"
  },
  "scripts": {
    "deploy": "npx prisma deploy",
    "generate": "npx prisma generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run generate",
    "build": "babel src -d build",
    "postbuild": "cd src && copy ./api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  }
}
